# vim: foldmethod=marker foldlevel=0 ft=cfg

# Time
regexp=\sup(?:\s(\d+ days?),)?\s+(\d+ min|\d+:\d+),
colours=default,yellow,green
-
# users
regexp=\s+(\d+)\susers
colours=yellow,bold yellow
-
# load average
regexp=load average: (\d+\.\d+),\s(\d+\.\d+),\s(\d+\.\d+)
colours=default,bright_cyan,cyan,bright_cyan
-
# W Command section
# Title
regexp=^USER.*$
colours=bold
